		Motorbike honda = new Motorbike();
		honda.move();
		
		Motorbike kawasaki=new Motorbike();
		kawasaki.trick();
		
//		System.out.println(honda.speed());

		

		
		
		
		
		Vehicle aCar = new Car();
		aCar.move();
		
		

	}

}
