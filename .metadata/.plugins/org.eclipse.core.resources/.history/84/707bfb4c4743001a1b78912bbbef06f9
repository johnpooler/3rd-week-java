package com.qa.garage;

public class Motorbike extends Vehicle{
	
	public Motorbike(String colour) {
		super(colour);
		
	}
	@Override
	public void move() {
		this.speed+=3;
		System.out.println("miooooooo");
	}
	public void trick(int altitude) {
		this.altitude+=2;
		System.out.println("that was legitness");
	}
	
}
