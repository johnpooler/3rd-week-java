package com.qa.garage;

public class Car extends Vehicle{
	public Car(String colour) {
		super(colour);
		
	}
	private int gas= 100;
	@Override
	public void move() {
		this.speed+=5;
		System.out.println("bruuuum");
	}
	public int getGas() {
		return gas;
	}
	public void setGas(int gas) {
		this.gas = gas;
	}
	
}
